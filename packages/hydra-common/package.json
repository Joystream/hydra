    "test": "nyc --extension .ts mocha --timeout 50000 --require ts-node/register --forbid-only \"src/**/*.test.ts\""
  },
  "dependencies": {
    "bn.js": "^4.12.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/bn.js": "^4.11.6",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.172"
  }
}