{
  "name": "@subsquid/hydra-common",
  "version": "4.1.0-alpha.4",
  "description": "Common Hydra tools",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "pub": "yarn build && yarn publish --access public",
